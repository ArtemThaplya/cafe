# CREATE SCHEMA `waiter` DEFAULT CHARACTER SET utf8 ;

CREATE TABLE `waiter`(
  `id_waiter` INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
  `first_name` VARCHAR(45) NOT NULL,
  `last_name` VARCHAR(45) NOT NULL,
  `age` VARCHAR(45) NOT NULL,
  `employment_date` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`id_waiter`))
  ENGINE = InnoDB
  DEFAULT CHARACTER SET = utf8;

